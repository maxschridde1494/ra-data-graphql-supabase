{
    "name": "ra-data-graphql-simple",
    "version": "5.1.4",
    "description": "A GraphQL simple data provider for react-admin",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/cjs/index.d.ts",
    "sideEffects": false,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/marmelab/react-admin.git"
    },
    "bugs": {
        "url": "https://github.com/marmelab/react-admin/issues"
    },
    "homepage": "https://github.com/marmelab/react-admin#readme",
    "authors": [
        "Gildas Garcia"
    ],
    "keywords": [
        "reactjs",
        "react",
        "react-admin",
        "admin-on-rest",
        "rest",
        "graphql"
    ],
    "license": "MIT",
    "scripts": {
        "build": "yarn run build-cjs && yarn run build-esm",
        "build-cjs": "rimraf ./dist/cjs && tsc --outDir dist/cjs",
        "build-esm": "rimraf ./dist/esm && tsc --outDir dist/esm --module es2015",
        "lint": "eslint --ext .js,.ts,.tsx \"./packages/**/src/**/*.{js,ts,tsx}\" \"./examples/**/src/**/*.{js,ts,tsx}\" \"./cypress/**/*.{js,ts,tsx}\"",
        "prettier": "prettier --config ./.prettierrc.js --write --list-different \"packages/*/src/**/*.{js,json,ts,tsx,css,md}\" \"examples/*/src/**/*.{js,ts,json,tsx,css,md}\" \"cypress/**/*.{js,ts,json,tsx,css,md}\"",
        "test-unit": "cross-env NODE_ENV=test cross-env BABEL_ENV=cjs NODE_ICU_DATA=./node_modules/full-icu jest",
        "watch": "tsc --outDir dist/esm --module es2015 --watch"
    },
    "dependencies": {
        "@apollo/client": "^3.3.19",
        "graphql": "^16.9.0",
        "graphql-ast-types-browser": "~1.0.2",
        "lodash": "~4.17.5",
        "pluralize": "~7.0.0",
        "ra-data-graphql": "^5.1.4",
        "react-admin": "^5.1.4"
    },
    "devDependencies": {
        "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
        "@testing-library/jest-dom": "^6.5.0",
        "@types/jest": "^29.5.13",
        "@types/react": "^18.3.3",
        "@typescript-eslint/eslint-plugin": "^5.60.0",
        "@typescript-eslint/parser": "^5.60.0",
        "cross-env": "^5.2.0",
        "eslint": "^8.19.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-config-react-app": "^7.0.1",
        "eslint-plugin-cypress": "^2.12.1",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-testing-library": "^5.11.0",
        "full-icu": "~1.3.1",
        "global-jsdom": "^9.0.1",
        "jest": "^29.5.0",
        "jest-circus": "29.5.0",
        "jest-environment-jsdom": "^29.5.0",
        "jest-resolve": "29.5.0",
        "jest-watch-typeahead": "2.2.2",
        "lint-staged": "^13.0.3",
        "lolex": "~2.3.2",
        "prettier": "~3.2.5",
        "raf": "~3.4.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-router-dom": "^6.26.2",
        "rimraf": "^3.0.2",
        "ts-jest": "^29.2.5",
        "typescript": "^5.1.3",
        "whatwg-fetch": "^3.6.20"
    },
    "gitHead": "587df4c27bfcec4a756df4f95e5fc14728dfc0d7",
    "resolutions": {
        "@typescript-eslint/typescript-estree": "^5.60.0",
        "@typescript-eslint/parser": "^5.60.0",
        "@typescript-eslint/eslint-plugin": "^5.60.0",
        "minimist": "^1.2.6",
        "ejs": "^3.1.7"
    }
}
